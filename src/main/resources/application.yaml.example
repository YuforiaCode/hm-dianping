server:
  port: 8081
spring:
  application:
    name: hmdp  #请替换为实际数据库名(正常情况创建数据库建议命名为hmdp)
  datasource:
    driver-class-name: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql://192.168.100.128:3306/hmdp?useSSL=false&serverTimezone=UTC  #请替换为实际数据库url地址
    username: root
    password: #请替换为实际密码
  data:
    redis:
      host: 192.168.100.128  #请替换为实际Redis地址
      port: 6379
      password: #请替换为实际密码
      lettuce:
        pool:
          max-active: 10
          max-idle: 10
          min-idle: 1
          time-between-eviction-runs: 10s
  jackson:
    default-property-inclusion: non_null # JSON处理时忽略非空字段
mybatis-plus:
  type-aliases-package: com.hmdp.entity # 别名扫描包
logging:
  level:
    com.hmdp: debug